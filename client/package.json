{
  "name": "client",
  "version": "1.0.0",
  "description": "VS Code extension client",
  "main": "./out/extension.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "build": "npm run compile",
    "lint": "eslint src --ext ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:yaml",
    "onInlineSuggestion"
  ],
  "enabledApiProposals": [
    "inlineCompletionsAdditions"
  ],
  "contributes": {
    "commands": [
      {
        "command": "buildoutlens.createServiceTemplate",
        "title": "Create Service Template"
      },
      {
        "command": "buildoutlens.annotate",
        "title": "AI Code Annotations"
      }
    ]
  },
  "dependencies": {
    "vscode-languageclient": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^18.19.112",
    "@types/vscode": "^1.101.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "typescript": "^5.8.3"
  }
}
